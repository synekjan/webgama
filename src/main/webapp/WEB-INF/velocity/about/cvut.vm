<!DOCTYPE html>
<html>
<head>
#parse("include.vm")
<title>#springMessage("about.organization")</title>
</head>
<body>
	<div id="wrapper">
		#parse("header.vm")
		<div id="container">
			<div id="about_container">
				<div class="right_menu">
					<ul>
						<li><a href="#springUrl("/about/project")">#springMessage("about.project")</a></li>
						<span class="active_menu"><li><a href="#springUrl("/about/cvut")">#springMessage("about.organization")</a></li></span>
						<li><a href="#springUrl("/about/author")">#springMessage("about.author")</a></li>
						<li><a href="#springUrl("/about/thanks")">#springMessage("about.thanks")</a></li>
						<li><a href="#springUrl("/about/contact")">#springMessage("about.contact")</a></li>
					</ul>
				</div>
				<div id="about_content">
					<div class="panel_header">
						<h2>#springMessage("about.organization")</h2>
					</div>
					<div class="content">
						<h1>#springMessage("about.organization.ctu")</h1>
						<p>#springMessage("about.organization.ctu.message")</p>
						<h1>#springMessage("about.organization.faculty")</h1>
						<p>#springMessage("about.organization.faculty.message")</p>
						<h1>#springMessage("about.organization.department")</h1>
						<p>#springMessage("about.organization.department.message")</p>
					</div>
				</div>
			</div>
		</div>
	</div>
	#parse("footer.vm")
</body>
</html>