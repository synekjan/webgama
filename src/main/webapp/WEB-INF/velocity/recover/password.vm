<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
#parse("unsec_include.vm")
<title>Registration</title>
</head>
<body>
	<div id="wrapper">
		#parse("unsec_header.vm")
		<div id="container">
			<div id="signup_container">
				<div class="panel_header">
					<h2>Reset Password</h2>
				</div>
				<div id="dialog_container">
					<p>Please enter the Username you registered with. New generated
						password will be sent via email shortly.</p>

					<form action="" method="post">
						<label for="username">Username:</label>
						#springFormInput("user.username", "") #springShowErrors("&nbsp",
						"form_errors") <br> <br> <input type="submit"
							value="Reset">
					</form>
				</div>
			</div>
		</div>
	</div>
	#parse("footer.vm")
</body>
</html>