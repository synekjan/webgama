												#springFormInput("input.clusters[$h].observations[$i].zenithAngles[$j].toDh",
												"")#showElementErrors()</div>
										</div>
									</fieldset>
									#end
								</fieldset>
								#end

								<!-- COORDINATES -->
								#elseif ($cluster.getCoordinates().size() != 0)
								<fieldset>
									<legend>#springMessage("adjustment.onepage.coordinates")</legend>
									#foreach ($coordinate in $cluster.coordinates)
									<fieldset>
