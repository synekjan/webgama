<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
#parse("include.vm")
<title>#springMessage("logins.list")</title>
</head>
<body>
	<div id="wrapper">
		#parse("header.vm")

		<div id="breadcrumbs">
			<a href="#springUrl("/")">WebGama</a> > <a href="#springUrl("/user/info")">#springMessage("user.settings")</a> > <a href="#springUrl("/user/logins/show")"><span class="active">#springMessage("logins.list")</span></a>
		</div>

		<div class="left_menu">
			<ul>
				<li><a href="#springUrl("/user/info")">#springMessage("user.general.settings")</a></li>
				<li><a href="#springUrl("/user/password/change")">#springMessage("change.password")</a></li>
				<li><a href="#springUrl("/user/logins/show")">#springMessage("logins.list")</a></li>
				<li><a href="#springUrl("/user/delete")">#springMessage("delete.account")</a></li>
			</ul>
		</div>

		<div id="container">

			<h2>#springMessage("logins.list")</h2>

			<table class="table">
				<thead>
					<tr>
						<th>#springMessage("logins.ip.address")</th>
						<th>#springMessage("logins.time")</th>
						<th>#springMessage("logins.success")</th>
					</tr>
				</thead>
				<tbody>
					#foreach ($login in $loginList)
					<tr>
						<td>$login.ip</td>
						<td>$login.time</td>
						<td>$login.success</td>
					</tr>
					#end
				</tbody>
			</table>


		</div>
		#parse("footer.vm")
	</div>
</body>
</html>