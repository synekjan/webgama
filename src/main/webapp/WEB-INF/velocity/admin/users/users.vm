<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
#parse("include.vm")
<title>Webgama| Users</title>
</head>
<body>
	<div id="wrapper">
		#parse("header.vm")
		<div id="container">
			<div id="account_container">
				<div class="left_menu">
					<ul>
						<span class="active_menu"><li><a href="#springUrl("/admin/users")">#springMessage("admin.section.users")</a></li></span>
						<li><a href="#springUrl("/admin/users/delete")">#springMessage("admin.section.users.delete")</a></li>
					</ul>
				</div>
				<div id="info" class="panel">
					<div class="panel_header">
						<h2>#springMessage("admin.section.users")</h2>
					</div>
					<table class="table">
						<thead>
							<tr>
								<th>ID</th>
								<th>Username</th>
								<th>Enabled</th>
								<th>E-mail</th>
								<th>First Name</th>
								<th>Last Name</th>
								<th>Telephone</th>
								<th>Street</th>
								<th>Number</th>
								<th>City</th>
								<th>Zip Code</th>
								<th>State</th>
							</tr>
						</thead>
						<tbody>
							#foreach ($user in $userList)
							<tr>
								<td>$!user.id</td>
								<td>$!user.username</td>
								<td>$!user.enabled</td>
								<td>$!user.email</td>
								<td>$!user.firstName</td>
								<td>$!user.lastName</td>
								<td>$!user.telephone</td>
								<td>$!user.street</td>
								<td>$!user.number</td>
								<td>$!user.city</td>
								<td>$!user.zipCode</td>
								<td>$!user.state</td>
							</tr>
							#end
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
	#parse("footer.vm")
</body>
</html>